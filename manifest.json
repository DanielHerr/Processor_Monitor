{
  "manifest_version": 3,
  "name": "Processor Monitor",
  "version": "11.2.3.1",
	"version_name": "11.2 (Mv3, Chrome 88+)",
  "description": "Displays CPU Usage Information",
  "icons": { "128": "icon.png" },
  "action": { "default_icon": "icon.png" },
  "author": "Daniel Herr",
  "background": { "service_worker": "background.js" },
  "externally_connectable": { "ids": [ "kbilomlpmhhhaimaigidhnjijhiajbam" ] },
  "homepage_url": "https://danielherr.software/Processor_Monitor",
  "offline_enabled": true,
  "optional_host_permissions": [ "<all_urls>" ],
  "options_page": "options.html",
  "permissions": [ "system.cpu", "storage", "activeTab", "notifications", "contextMenus", "scripting" ]
}